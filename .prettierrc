{
  "plugins": [
    "prettier-plugin-astro",
    "@prettier/plugin-oxc",
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "overrides": [
    {
      "files": ["*.astro"],
      "parser": "astro"
    },
    {
      "files": ["*.js", "*.jsx"],
      "parser": "oxc"
    },
    {
      "files": ["*.ts", "*.tsx"],
      "parser": "oxc-ts"
    }
  ]
}
